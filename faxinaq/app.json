{
  "expo": {
    "name": "faxinaq",
    "slug": "faxinaq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9dc6fb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location for some feature."
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#9DC6FB",
        "permissions": ["ACCESS_FINE_LOCATION"],
        "usesPermissions": ["android.permission.ACCESS_FINE_LOCATION"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
  }}
